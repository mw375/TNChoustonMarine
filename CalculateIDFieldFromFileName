import arcpy

arcpy.env.workspace = r"E:\0Val_Local\Phase2DataCleaning\6_SpeedFiltered_PointsData\OSPR.gdb"
fcs = arcpy.ListFeatureClasses()

for fc in fcs:
    if 'Drexel' in fc and 'NULLGEOM' in fc:
        idnum = str(fc[:-14])
        print idnum
        arcpy.AddField_management(fc,'ID_final','TEXT')
        arcpy.CalculateField_management(fc,'ID_final','"'+idnum+'"','PYTHON')
