import arcpy

arcpy.env.workspace = r"E:\0Val_Local\Phase2DataCleaning\2_PointsData_Fixed\OSPR.gdb"
arcpy.env.overwriteOutput = True

fcs = arcpy.ListFeatureClasses()

outpath = "E:/0Val_Local/Phase2DataCleaning/2_PointsData_Fixed/OSPR.gdb/"


for fc in fcs:
    if fc.startswith('VAaqua'):
        print fc
        FL = arcpy.MakeFeatureLayer_management(fc)
        arcpy.SelectLayerByAttribute_management(FL,"NEW_SELECTION","location_lat is not null")
        arcpy.CopyFeatures_management(FL,outpath+'{}_NullsRemoved'.format(fc))

print "nulls deleted"

arcpy.env.workspace = outpath
fcs2 = arcpy.ListFeatureClasses()

if len(fcs) == len(fcs2):
    print 'yes, same amount of outputs as inputs'
else:
    print 'error - not as many outputs as inputs'

print 'inputs:'+str(len(fcs))
print 'outputs:'+str(len(fcs2))
